# Abaikan folder dependensi (ini di-install ulang)
/vendor/
/node_modules/

# Abaikan file Rahasia
# INI YANG PALING PENTING!
.env

# Abaikan file log Laravel
/storage/logs/*.log
/storage/framework/cache/
/storage/framework/sessions/
/storage/framework/views/

# Abaikan aset yang sudah di-compile (ini dibuat ulang)
/public/build/
/public/hot

